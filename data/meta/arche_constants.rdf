            <acdh:hasCoverageStartDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date">1750-07-08</acdh:hasCoverageStartDate>
            <acdh:hasCoverageEndDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date">1798-09-01</acdh:hasCoverageEndDate>
